var FbActions;

(function(){
  
  FbActions = this.FbActions = new Class({
    
    Implements: [Options, Events],
    
    options: {},
    
    initialize: function(selector, options){
      this.selector = selector;
      this.setOptions(options);
      this.mainCont = document.id('content');
      if (this.mainCont){
        this.attach();
      }
    },
    
    attach: function(){
      this.mainCont.addEvent('click:relay(' + this.selector + ')', this.trigger.bind(this));
    },
    
    trigger: function(event, element){
      event.stop();
      this[element.get('data-event-type').camelCase()].call(this, element);
    },
    
    showConfirmation: function(){
      this.empty();
      Elements.from('<li class=\"requested\">Request sent</li>').inject(this);
    },
    
    requestEndorsement: function(element){
      this.endorseWith[JT_config.fb.post_type.camelCase()].call(this, element);
    },
    
    endorseWith: {
      
      wallPost: function(element){
        var request_endorsement = element.getParent('.request_endorsement');
        new Request.JSON({
          url: element.get('href'),
          method: 'post',
          data: {
            fb_id: element.data('fb-id'),
            post_type: JT_config.fb.post_type
          },
          onRequest: function(){
            request_endorsement.getElement('li').hide();
          },
          onComplete: function(response){
            this.showConfirmation.call(request_endorsement);
            this.fireEvent('onRequestedEndorsement', element, response);
          }.bind(this)
        }).send();
      },
      
      privateMessage: function(element){
        var request_endorsement = element.getParent('.request_endorsement');
        if (request_endorsement){
          request_endorsement.getElement('li').hide();
        }
        FB.ui({
            method: 'send',
            to: element.data('fb-id'),
            name: 'Jobandtalent',
            link: 'http://jobandtalent.com',
            picture: 'http://fbrell.com/f8.jpg',
            description: 'Hi, {name} would like you to endorse him at Jobandtalent'
          },
          function(response){
            if (response && response.post_id){
              this.fireEvent('onRequestedEndorsement', element);
              this.showConfirmation.call(request_endorsement);
            } else {
              request_endorsement.getElement('li').show();
            }
          }.bind(this)
        );
      }
    },
    
    request: function(opt){
      new Request({
        url: opt.element.get('href'),
        method: 'post',
        data: {
          displayed_ids: opt.cont.getParent('ul').getElements('.skip a').get('data-relationship-id')
        },
        onRequest: function(){
          opt.element.getParent('.fbf_actions').getElements('a').hide();
        },
        onComplete: function(response){
          this.skip(opt.element, {
            templateString: response,
            templateNodes: Elements.from(response)
          });
        }.bind(this)
      }).send();
    },
    
    fetch: function(element){
      this.request.call(this, {
        element: element,
        cont: element.getParent('.fbf_cont')
      });
    },
    
    skip: function(element, response){
      var cont = element.getParent('.fbf_cont');
      if (response.templateString === ''){
        cont.getParent().dispose();
      } else {
        new Fx.Tween(cont, {
          onComplete: this.showNext.bind(this, element, response)
        }).start('left', [0, -285]);
      }
    },
    
    showNext: function(element, response){
      var cont = element.getParent('.fbf_cont');
      cont.empty();
      cont.adopt(response.templateNodes);
      cont.tween('left', [285, 0]);
    }
    
  });
  
}).call();
